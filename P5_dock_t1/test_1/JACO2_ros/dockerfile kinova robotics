FROM osrf/ros:noetic-desktop
RUN apt-get update
RUN apt-get install git -y
RUN sudo apt-get install apt-transport-https -y
RUN mkdir catkin_ws
RUN cd catkin_ws
RUN mkdir src
RUN cd src
RUN apt-get update
RUN git clone -b noetic-devel git@github.com:Kinovarobotics/kinova-ros.git kinova-ros -y
